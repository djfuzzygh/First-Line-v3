{
  "name": "firstline-triage-platform",
  "version": "1.0.0",
  "description": "Multi-channel AI-powered clinical decision-support system for low-resource environments",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/local-server.ts",
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "test:properties": "jest src/tests --runInBand --testPathPattern=property.test.ts --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --passWithNoTests",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "preflight": "scripts/preflight.sh",
    "kaggle:smoke": "python3 kaggle/smoke_test.py",
    "deploy:gcp": "scripts/deploy-gcp.sh",
    "cdk": "cd infrastructure && cdk",
    "cdk:synth": "cd infrastructure && cdk synth",
    "cdk:deploy": "cd infrastructure && cdk deploy",
    "cdk:diff": "cd infrastructure && cdk diff",
    "cdk:destroy": "cd infrastructure && cdk destroy",
    "deploy:dev": "infrastructure/scripts/deploy.sh dev us-east-1",
    "deploy:staging": "infrastructure/scripts/deploy.sh staging us-east-1",
    "deploy:prod": "infrastructure/scripts/deploy.sh prod us-east-1"
  },
  "keywords": [
    "healthcare",
    "triage",
    "aws",
    "serverless",
    "ai"
  ],
  "author": "",
  "license": "CC-BY-4.0",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.700.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.991.0",
    "@aws-sdk/client-dynamodb": "^3.700.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/client-secrets-manager": "^3.992.0",
    "@aws-sdk/client-sns": "^3.700.0",
    "@aws-sdk/lib-dynamodb": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.991.0",
    "@aws-sdk/util-dynamodb": "^3.991.0",
    "@google-cloud/firestore": "^8.3.0",
    "@google-cloud/secret-manager": "^6.1.1",
    "@google-cloud/storage": "^7.19.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/pdfkit": "^0.17.5",
    "aws-cdk-lib": "^2.170.0",
    "bcryptjs": "^3.0.3",
    "constructs": "^10.4.2",
    "express": "^4.21.1",
    "pdfkit": "^0.17.2",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/aws-lambda": "^8.10.160",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "aws-cdk": "^2.170.0",
    "aws-sdk-client-mock": "^4.1.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fast-check": "^3.23.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
