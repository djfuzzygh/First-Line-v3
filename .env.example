# Runtime
NODE_ENV=development
PORT=8080

# Data/storage
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1
FIRESTORE_COLLECTION=FirstLineData
GCS_BUCKET=your-gcs-bucket

# AI provider selection: vertexai | kaggle | huggingface | bedrock
AI_PROVIDER=vertexai

# Vertex AI
VERTEXAI_MODEL_ID=medgemma-2b
# Optional local auth helpers:
# GCP_ACCESS_TOKEN=
# GCP_SERVICE_ACCOUNT_JSON=

# Kaggle provider (used when AI_PROVIDER=kaggle)
KAGGLE_INFER_URL=https://your-kaggle-endpoint/infer
KAGGLE_API_KEY=
KAGGLE_MODEL_NAME=medgemma-kaggle

# Hugging Face provider (used when AI_PROVIDER=huggingface)
HF_INFER_URL=
HF_API_TOKEN=
HF_MODEL_ID=google/medgemma-2b-it

# Optional local/dev behaviors
FIRESTORE_IN_MEMORY=false
DISABLE_SECRET_MANAGER=false
HEALTHCHECK_DEEP=false
HEALTHCHECK_TIMEOUT_MS=4000
HEALTHCHECK_VERTEX_LIVE=false
